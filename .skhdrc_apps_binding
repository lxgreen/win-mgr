                              && code ~/rich-content\
                              && /Applications/Kitty.app/Contents/MacOS/kitty -1 --instance-group dev -d ~/rich-content\
                              && /Applications/Kitty.app/Contents/MacOS/kitty -1 --instance-group dev -d ~/rich-content/examples/main
